    <selectionEntry type="upgrade" import="true" name="Bailout" hidden="false" id="2cd2-087b-e9a1-6072">
      <infoLinks>
        <infoLink name="Bailout" id="Bailout" hidden="false" type="rule" targetId="e024-3b5a-3d53-c103"/>
      </infoLinks>
      <constraints>
        <constraint type="min" value="1" field="selections" scope="parent" shared="true" id="103b-def3-aed1-41f3-min" includeChildSelections="false"/>
        <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="103b-def3-aed1-41f3-max" includeChildSelections="false"/>
      </constraints>
      <costs>
        <cost name="Munitions" typeId="Points" value="10"/>
        <cost name="Supply" typeId="284c-5503-e866-a2e0" value="0"/>
